{
  "name": "js_desk",
  "version": "1.0.0",
  "description": "An Electron application with React and Node",
  "main": "./main.js",
  "homepage": "http://./",
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && npm run electron-dev\" \"nodemon server/server.js\"",
    "dev": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && npm run electron-dev\" \"npm run server-start\"",
    "build": "react-app-rewired build",
    "react-start": "react-app-rewired start",
    "server-build": "babel server --out-dir build-server",
    "server-start": "nodemon server/init.js --exec babel-node server/server.js",
    "electron-dev": "node scripts/dev.js scripts/start.js",
    "electron-pack": "electron-builder -c.extraMetadata.main='./main.js'",
    "preelectron-pack": "npm run build && npm run server-build",
    "electron-react": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && npm run electron-dev\"",
    "pm2-start": "pm2 start worker.config.js --name ontax_worker"
  },
  "author": "tbuyse",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.4",
    "concurrently": "^4.1.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^0.2.14",
    "electron": "^5.0.13",
    "electron-builder": "^20.41.0",
    "react-app-rewired": "^2.2.1",
    "wait-on": "^3.2.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.13.1",
    "core-js": "^3.23.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "electron-is-dev": "^1.1.0",
    "electron-wix-msi": "^5.1.3",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.9",
    "piping": "^1.0.0-rc.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.23.1",
    "regenerator-runtime": "^0.13.9",
    "styled-components": "^5.3.3",
    "unzipper": "^0.12.1"
  },
  "build": {
    "appId": "com.electron-react-node",
    "files": [
      "build/**/**/*",
      "build-server/**/**/*",
      "node_modules/**/*",
      "./main.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "icon": "./ico.ico"
    }
  },
  "browserslist": [],
  "license": "ISC"
}
